/* pages/duzi/duzi.wxss */
page{
  background: #CCEEFF;
}
.head_BT{
  width: 90%;height: 60rpx;position: relative;
  margin: 0 auto;border-bottom:2rpx solid #6666FF;
}
.headbt{
  display: block;padding-top:10rpx; 
  color: 	#6666FF;font-weight: 900;
}
.box{
  width: 100%;position: relative;margin: 0 auto;
  overflow: hidden;margin-top: -70rpx;
}
.SP{
  width: 100%;margin-top: 100rpx;
}
.SP_bb{
  margin-top: 10rpx;margin: 0 auto;background: white;
  width: 95%;height: 500rpx;border-radius: 30rpx;
}
.SP_bt{
  width: 100%;height: 45rpx;border-bottom:1rpx solid #CCEEFF;
  overflow: hidden;text-overflow:ellipsis;white-space: nowrap;padding-top:5rpx; 
}
.SP_btnro{
  color: #3300CC;font-size: 26rpx; font-weight: 600;
}
.SP_nro{  margin: 0 auto;
  width: 99%;height: 350rpx;background: black;
  margin-top: 2rpx;overflow: hidden;border-radius: 30rpx;
}
.SP_nro video{
  display: block;margin: 0 auto;height: 100%;
}
.SP_mix{
  width:100%;height: 105rpx;
}
.toux-img{
  width: 80rpx;height: 80rpx;border-radius: 50%;float: left;
  margin-top:8rpx;border: 4rpx solid #FF88C2;overflow: hidden
}
.toux-img image{
  width: 100%;height: 100%;
}
.yhname{
  float: left;display: block;margin-top: 30rpx;
  font-weight: 900; color: #0066FF;margin-left: 10rpx;
}
.getime{
  float: right;margin-top: 60rpx;font-size: 25rpx;
  margin-right: 25rpx;color: #3366FF;
}